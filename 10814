import java.util.Arrays;
import java.util.StringTokenizer;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.io.IOException;

public class AACJ
{
	public static class People implements Comparable<People>
	{
		public int age;
		public String name;
		
		public static int count;
		
		public int order;
	
		public People () { age = 0; name = ""; count++; this.order = count; }
		
		public People (int age, String name) { this.age = age; this.name = name; count++; this.order = count; }
		
		public String toString() { return this.age + " " + this.name; }
		
		@Override
		public int compareTo(People p) 
		{
			if ( this.age > p.age )
				return 1;
			
			else if ( this.age < p.age)
				return -1;
			
			else
			{
				if (this.order > p.order)
					return 1;
				else
					return -1;
			}
		}
	}
	
	public static void main (String[] args) throws IOException
	{
		BufferedReader input = new BufferedReader( new InputStreamReader(System.in) );
		BufferedWriter out = new BufferedWriter( new OutputStreamWriter(System.out) );
		
		int n = Integer.parseInt( input.readLine() );
		
		People[] pArr = new People[n];
		
		for (int i=0; i<n; i++)
		{
			StringTokenizer st = new StringTokenizer( input.readLine(), " " );
			
			int a = Integer.parseInt( st.nextToken() );
			String b = st.nextToken();
			
			pArr[i] = new People(a,b);
		}
		
		Arrays.sort(pArr);
		
		for (int i=0; i<n; i++)
			out.write( pArr[i] + "\n");
		
		out.flush();
		out.close();
	
	}	
		
}
