// 조합 알고리즘 참조 https://drcode-devblog.tistory.com/304
// 병신코드, 시간초과, 머리가 나쁘면, 뇌가 고생하고 시간도 날린다

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.util.StringTokenizer;
import java.util.HashMap;
import java.util.Set;
import java.util.ArrayList;

public class Main
{
	static int count = 0;
	
	public static void main (String[] args) throws IOException
	{
		BufferedReader input = new BufferedReader( new InputStreamReader(System.in) );
		BufferedWriter out = new BufferedWriter( new OutputStreamWriter(System.out) );

		int T = Integer.parseInt(input.readLine());
        
        int n, m;
        
		while (T-- > 0)
		{
			HashMap<String, Integer> hm = new HashMap<String, Integer>();
			
			int N = Integer.parseInt(input.readLine());
			
			
			
			for (int i=0; i<N; i++)
			{
				StringTokenizer st = new StringTokenizer(input.readLine());
				
				st.nextToken();
				String b = st.nextToken();
				
				if ( hm.containsKey(b) == false)
					hm.put(b, 1);
				else
					hm.replace(b, hm.get(b) + 1);
			}
			
			ArrayList<String> keyList = new ArrayList<String>(hm.keySet());
            
			for (int i=1; i<=hm.size(); i++)
			{

				n = hm.size();
				m = i;
				
				int[] combi = new int[m];
				
				dfs(0, 0, combi, n, m, hm, keyList);
				
			}
			
			out.write(count + "");
			
			out.flush();
			
		}
		
		out.close();
		
	}
	
	
		
	public static void dfs(int L, int s, int[] combi, int n, int m, HashMap<String, Integer> hm, ArrayList<String> keyList) 
	{
		if(L == m) 
		{
			int temp = 1;
			
			for(int x : combi) 
			{
				temp *= hm.get(keyList.get(x));
			}
			
			count += temp;
		} 
		else 
		{
			for (int i = s; i < n; i++) 
			{
				combi[L] = i;
				dfs(L+1, i+1, combi, n, m, hm, keyList);
			}
		}
	}
}
