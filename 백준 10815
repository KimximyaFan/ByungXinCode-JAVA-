// 이분 탐색
// 인자에 넣을 때, 어레이 크기 - 1 해야하는거 유의

import java.util.Arrays;

import java.util.StringTokenizer;
import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.io.IOException;

public class AACI 
{
	public static void main (String[] args) throws IOException
	{
		BufferedReader input = new BufferedReader( new InputStreamReader(System.in) );
		BufferedWriter out = new BufferedWriter( new OutputStreamWriter(System.out) );
		
		int n = Integer.parseInt( input.readLine() );
		int[] A = new int[n];
		
		StringTokenizer st1 = new StringTokenizer( input.readLine(), " " );
		
		for (int i=0; i<n; i++)
		{
			A[i] = Integer.parseInt( st1.nextToken() );
		}
		
		Arrays.sort(A);
		
		int m = Integer.parseInt( input.readLine() );
		int[] B = new int[m];
			
		StringTokenizer st2 = new StringTokenizer( input.readLine(), " " );
		
		for (int i=0; i<m; i++)
		{
			B[i] = Integer.parseInt( st2.nextToken() );
		}
		
		for (int i=0; i<m; i++)
		{
			if ( binarySearch(A, 0, A.length - 1, B[i]) == true )
				out.write("1" + " ");
			else
				out.write("0" + " ");
		}
		
		out.flush();
		out.close();
	}
	
	public static boolean binarySearch(int[] A, int a, int b, int c)
	{
		int mean = (a + b) / 2;
		
		if ( a <= b)
		{
			if ( c > A[mean] )
				return binarySearch( A, mean+1, b, c);
			else if ( c < A[mean] )
				return binarySearch( A, a, mean-1, c);
			else
				return true;
		}
		
		return false;

	}
		
}
