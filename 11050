import java.util.Scanner;

public class Main
{
	public static void main (String[] args)
	{
		Scanner input = new Scanner(System.in);
		
		int n = input.nextInt();
		int k = input.nextInt();
		
		System.out.print( f(n,k) );
	}
	
	public static int f (int n, int k)
	{
		if (k == 0 || n == k)
			return 1;
		
		if ( k > n/2 )
			k = n-k;
		
		int N = 1;
		int K = 1;
		
		for (int i=0; i<k; i++)
		{
			N *= n-i;
			K *= k-i;
		}
			
		return N/K;
	}
}
